= link_to 'Admin Check', '#', class: 'toggleable', data: { toggleable: '.admin-process-check' }

.admin-process-check
  %h2
    Considerations
  - @patient_case.considerations.each do |consideration|
    = consideration.title

  %h2
    Available Medications
  - @patient_case.potential_medications.each do |medication|
    %p
      = medication.title
      = medication.age_min
      = medication.age_max
      = medication.gender

  %h2
    Available Diagnosis
  - @patient_case.potential_diagnosis.each do |diagnosis|
    %p
      = diagnosis.title
      = diagnosis.age_min
      = diagnosis.age_max
      = diagnosis.gender