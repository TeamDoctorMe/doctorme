.container
  %p
    %b Title:
    = @medication.title
  %p
    %b Description:
    = @medication.description
  %p
    %b Age min:
    = @medication.age_min
  %p
    %b Age max:
    = @medication.age_max
  %p
    %b Gender:
    = @medication.gender

  %p
    %b Permitted Exclusions:
    - @medication.considerations.each do |consideration|
      = consideration.title

  = link_to 'Edit', edit_admin_medication_path(@medication)
